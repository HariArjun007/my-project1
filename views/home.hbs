<h3>STUDENT MANAGEMENT SYSTEM</h3>
<p><a href="/adduser" class="addbtn">Add User</a></p>


<table>
    <thead>
        <tr>
            <td>#</td>
            <td>Name</td>
            <td>Age</td>
            <td>City</td>
            <td>Email</td>
            <td>DOB</td>
            <td>Education</td>
            <td>Action</td>
            <td>delete</td>
           
        </tr>
    </thead>
    <tbody>
    {{#each rows}}
       <tr>
        <td>{{this.ID}}</td>
        <td>{{this.NAME}}</td>
        <td>{{this.AGE}}</td>
        <td>{{this.CITY}}</td>
        <td>{{this.EMAIL}}</td>
        <td>{{this.DOB}}</td>
        <td>{{this.EDUCATION}}</td>
        <td><a class="Edit" href="/edituser/{{this.ID}}">Edit</a></td>
      <td><a onclick="return confirm('Are You sure you want to delete ?')" class="Delete" href="/deleteuser/{{this.ID}}">Delete</a></td>
       </tr>
       

    {{/each}}
       
    </tbody>
</table>